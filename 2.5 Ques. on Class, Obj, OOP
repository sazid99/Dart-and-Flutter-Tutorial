class and object part: 
ans. 1:
class Car {
  String? brand;
  String? model;
  int? year;

  Car(String brand, String model, int year) {
    this.brand = brand;
    this.model = model;
    this.year = year;
  }
}

void main() {
  var car = Car("Rang Rover", "RXGT5", 2024);
  print(car.brand);
}

