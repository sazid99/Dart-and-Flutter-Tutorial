Inheritance:
-------------
Ans. 1:
class Animal {
  void eat() {
    print("Animal eats ");
  }

  void sleep() {
    print("Animal sleeps");
  }
}

class Dog extends Animal {
  void eat() {
    print("Dog eats ");
  }

  void sleep() {
    print("Dog sleeps");
  }
}

class Cat extends Animal {
  void eat() {
    print("Cat eats ");
  }

  void sleep() {
    print("Cat sleeps");
  }
}

void main() {
  Dog dog = Dog();
  dog.eat();
  dog.sleep();

  Cat cat = Cat();
  cat.eat();
  cat.sleep();
}
-------------------------------------------------------------------------------

Ans. 2:
class Shape {
  void area() {}
}

class Rectangle extends Shape {
  var height;
  var width;

  Rectangle(this.height, this.width);
  @override
  void area() {
    var result = height * width;
    print(result);
  }
}

class Circle extends Shape {
  var radius;
  static const double PI = 3.14;

  Circle(this.radius);
  @override
  void area() {
    var result = PI * (radius * radius);
    print(result);
  }
}

void main() {
  Rectangle rectangle = Rectangle(10, 10);
  rectangle.area();

  Circle circle = Circle(10);
  circle.area();
}
---------------------------------------------------------------------
Ans. 3:
abstract class Vehicle {
  void wheel() {
    print("I have wheel");
  }
}

class Car extends Vehicle {
  @override
  void wheel() {
    print("I have 4 wheels");
  }
}

class Bike extends Vehicle {
  @override
  void wheel() {
    print("I have 2 wheels");
  }
}

void main() {
  Car car = Car();
  car.wheel();

  Bike bike = Bike();
  bike.wheel();
}
